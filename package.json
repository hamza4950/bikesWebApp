{
  "name": "react-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test:jest": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:unit": "node --experimental-vm-modules node_modules/jest/bin/jest.js --reporters='default' --reporters='jest-junit'",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --reporters='default' --reporters='jest-junit' --coverage",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js --reporters='default' --reporters='jest-junit' --coverage --coverageReporters='cobertura'"
  },
  "jest-junit": {
    "outputDirectory": "./testResults",
    "outputName": "TEST-RESULT.xml"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "@popperjs/core": "^2.11.8",
    "axios": "^1.6.7",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-junit-reporter": "^1.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.0",
    "react-router-dom": "^6.21.2",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "vite": "^5.0.8"
  }
}
